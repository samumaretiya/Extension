<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1.0 maximum-scale=1, user-scalable=no" />
		<meta name="format-detection" content="telephone=no" />
		<script src="js/modernizr.custom.min.js"/>
		<css src="mage/calendar.css"/>
		<css src="css/font-awesome.css"/>
		<css src="css/owl.carousel.css"/>	
        <script src="requirejs/require.js"/>
		<script src="js/jquery.uniform.min.js"/>
	    <script src="js/owl.carousel.min.js"/>
    </head>
    <body>
        <referenceContainer name="after.body.start">
            <block class="Magento\Framework\View\Element\Js\Components" name="head.components" as="components" template="Magento_Theme::js/components.phtml" before="-"/>
        </referenceContainer>
		<referenceContainer name="before.body.end">
    		  <block class="\Magento\Framework\View\Element\Template" template="Magento_Theme::js.phtml" name="footerjscss" cacheable="false"/>
		</referenceContainer>
    </body>
</page>
